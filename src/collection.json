      "description": "Adds dependencies to the application without affecting any templates",
      "factory": "./ng-add",
      "schema": "./ng-add/schema.json",
      "aliases": ["init"]
    },
    "plugin": {
      "description": "Add ThingsPro App UI.",
      "factory": "./plugin",
      "schema": "./plugin/schema.json"
    }
  }
}
